MEMEORY EXCHANGE


#include<stdio.h>
#include<stdlib.h>
#include<pic18f4550.h>

void main(void){
    int temp;
    int source[] = {0x10,0x20,0x30,0x40,0x50};
    int dest[] = {0x00,0x00,0x00,0x00,0x00};
    for(int i = 0;i<5;i++){
        temp = source[i];
        source[i]=dest[i];
        dest[i]=temp;
    }
}


MEMORY TRANSFER

include<stdio.h>
#include<stdlib.h>
#include<pic18f4550.h>

void main(void){
    int source[] = {0x10,0x20,0x30,0x40,0x50};
    int dest[] = {0x00,0x00,0x00,0x00,0x00};
    for(int i = 0;i<5;i++){
        dest[i] = source[i];
    }
}




